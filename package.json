{
  "name": "from-template",
  "version": "1.0.9",
  "description": "CLI + NPM Scripts tool for template directories.",
  "bin": {
    "from-template": "./bin/from-template.js"
  },
  "files": [
    "lib",
    "bin"
  ],
  "scripts": {
    "build": "nwb build-web-module",
    "clean": "nwb clean-module",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "prepublish": "npm run build",
    "link": "./link.sh"
  },
  "dependencies": {
    "babel-runtime": "6.26.0",
    "chalk": "2.4.2",
    "commander": "4.1.1",
    "stephenwf-fork-copy-template-dir": "1.3.0",
    "find-plugins": "1.1.7",
    "fs-extra": "8.1.0",
    "inquirer": "6.5.0",
    "inquirer-promise": "1.0.0",
    "optimist": "0.6.1",
    "pretty-error": "2.1.1",
    "webpack-node-externals": "1.7.2"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-jest": "23.6.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "latest",
    "babel-preset-es2015": "6.24.1",
    "coveralls": "3.0.5",
    "jest": "23.6.0",
    "nwb": "0.23.0",
    "regenerator-runtime": "0.13.2"
  },
  "jest": {
    "testMatch": [
      "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    }
  },
  "author": "Stephen Fraser <stephen.fraser@digirati.com>",
  "homepage": "",
  "license": "MIT",
  "repository": ""
}
